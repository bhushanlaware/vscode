{
	"name": "vscode-responsive-preview",
	"displayName": "vscode-responsive-preview",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"workspaceContains:**/challenge/public/index.html",
		"workspaceContains:**/challenge/src/index.html"
	],
	"browser": "./dist/web/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"type": "webview",
					"id": "hackerrank.resize-preview",
					"name": "Resize Preview"
				}
			]
		}
	},
	"scripts": {
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
		"pretest": "yarn run compile-web",
		"vscode:prepublish": "yarn run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
		"build-react": "webpack build --config ./webpack-react.config.js",
		"build-react-watch": "webpack build --config ./webpack-react.config.js --watch"
	},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/preset-env": "^7.16.4",
		"@babel/preset-react": "^7.16.0",
		"@babel/preset-typescript": "^7.16.0",
		"@types/mocha": "^9.0.0",
		"@types/vscode": "^1.62.0",
		"@types/webpack-env": "^1.16.2",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-web": "^0.0.15",
		"assert": "^2.0.0",
		"babel-loader": "^8.2.3",
		"css-loader": "^6.5.1",
		"eslint": "^8.1.0",
		"mini-css-extract-plugin": "^2.4.5",
		"mocha": "^9.1.3",
		"process": "^0.11.10",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.2.5",
		"typescript": "^4.4.4",
		"url-loader": "^4.1.1",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"@types/react-dom": "^18.0.2",
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	}
}